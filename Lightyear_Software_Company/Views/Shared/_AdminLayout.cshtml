@{
    if (Session["AdminID"] == null)
    {
        Response.Redirect("/Admin/Login/");
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Lightyear Admin Panel</title>
    <!--
     <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
     <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
     <script src="~/Scripts/modernizr-2.8.3.js"></script> -->

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Creative - Bootstrap 3 Responsive Admin Template">
    <meta name="author" content="GeeksLabs">
    <meta name="keyword" content="Creative, Dashboard, Admin, Template, Theme, Bootstrap, Responsive, Retina, Minimal">
    <link rel="shortcut icon" href="img/favicon.png">

    <title>Creative - Bootstrap Admin Template</title>

    <!-- Bootstrap CSS -->
    <link href="/Content/AdminPanelLayout/css/bootstrap.min.css" rel="stylesheet">
    <!-- bootstrap theme -->
    <link href="/Content/AdminPanelLayout/css/bootstrap-theme.css" rel="stylesheet">
    <!--external css-->
    <!-- font icon -->
    <link href="/Content/AdminPanelLayout/css/elegant-icons-style.css" rel="stylesheet" />
    <link href="/Content/AdminPanelLayout/css/font-awesome.min.css" rel="stylesheet" />
    <!-- full calendar css-->
    <link href="/Content/AdminPanelLayout/assets/fullcalendar/fullcalendar/bootstrap-fullcalendar.css" rel="stylesheet" />
    <link href="/Content/AdminPanelLayout/assets/fullcalendar/fullcalendar/fullcalendar.css" rel="stylesheet" />
    <!-- easy pie chart-->
    <link href="/Content/AdminPanelLayout/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <!-- owl carousel -->
    <link rel="stylesheet" href="/Content/AdminPanelLayout/css/owl.carousel.css" type="text/css">
    <link href="/Content/AdminPanelLayout/css/jquery-jvectormap-1.2.2.css" rel="stylesheet">
    <!-- Custom styles -->
    <link rel="stylesheet" href="/Content/AdminPanelLayout/css/fullcalendar.css">
    <link href="/Content/AdminPanelLayout/css/widgets.css" rel="stylesheet">
    <link href="/Content/AdminPanelLayout/css/style.css" rel="stylesheet">
    <link href="/Content/AdminPanelLayout/css/style-responsive.css" rel="stylesheet" />
    <link href="/Content/AdminPanelLayout/css/xcharts.min.css" rel=" stylesheet">
    <link href="/Content/AdminPanelLayout/css/jquery-ui-1.10.4.min.css" rel="stylesheet">

    <script src="~/Content/ckeditor/ckeditor.js"></script>
    <link href="~/Content/ckeditor/contents.css" rel="stylesheet" />

    <script src="/Content/AdminPanelLayout/js/jquery.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery-1.8.3.min.js"></script>

    @*<script src="~/Scripts/jquery-3.4.1.js"></script>*@
    <script src="~/Scripts/DataTables/jquery.dataTables.js"></script>
    <link href="~/Content/DataTables/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/dataTables.bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/DataTables/dataTables.bootstrap.js"></script>
</head>

<body>
    <!-- container section start -->
    <section id="container" class="">


        <header class="header dark-bg">
            <div class="toggle-nav">
                <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"><i class="icon_menu"></i></div>
            </div>

            <!--logo start-->
            <a href="/Admin/Index/" class="logo">Lightyear <span class="lite">Admin</span></a>
            <!--logo end-->



            <div class="top-nav notification-row">
                <!-- notificatoin dropdown start-->
                <ul class="nav pull-right top-menu">
                    <!-- alert notification start-->
                    <li id="alert_notificatoin_bar" class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">

                            <i class="icon-bell-l"></i>
                            <span class="badge bg-important">4</span>
                        </a>
                        <ul class="dropdown-menu extended notification">
                            <div class="notify-arrow notify-arrow-blue"></div>
                            <li>
                                <p class="blue">You have 4 new notifications</p>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="label label-primary"><i class="icon_profile"></i></span>
                                    Friend Request
                                    <span class="small italic pull-right">5 mins</span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="label label-warning"><i class="icon_pin"></i></span>
                                    John location.
                                    <span class="small italic pull-right">50 mins</span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="label label-danger"><i class="icon_book_alt"></i></span>
                                    Project 3 Completed.
                                    <span class="small italic pull-right">1 hr</span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class="label label-success"><i class="icon_like"></i></span>
                                    Mick appreciated your work.
                                    <span class="small italic pull-right"> Today</span>
                                </a>
                            </li>
                            <li>
                                <a href="#">See all notifications</a>
                            </li>
                        </ul>
                    </li>
                    <!-- alert notification end-->
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <span class="profile-ava">
                                <img alt="" src="~/Content/AdminPanelLayout/img/spiderman.png">
                            </span>
                            <span class="username">@Session["Mail"]</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <div class="log-arrow-up"></div>
                            <li class="eborder-top">
                                <a href="/Admin/Logout/"><i class="icon_key_alt"></i> Log Out</a>
                            </li>
       
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!-- notificatoin dropdown end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu">
                    <li class="active">
                        <a class="" href="/Admin/Index/">
                            <i class="icon_house_alt"></i>
                            <span>Dashboard</span>
                        </a>
                    </li>
                    @if (Session["Yetki"].ToString() == "Admin") 
                    {
                        <li>
                            <a class="" href="/WebsiteIdentity/Index/">
                                <!---<i class="fa fa-bookmark"></i>-->
                                <i class="icon_document_alt"></i>
                                <span>Identity</span>
                            </a>
                        </li>
                        <li>
                            <a class="" href="/About/Index/">

                                <i class="fa fa-info-circle"></i>
                                <span>About</span>
                            </a>
                        </li>
                        <li>
                            <a class="" href="/Contact/Index/">
                                <i class="fa fa-envelope"></i>
                                <span>Contact</span>
                            </a>
                        </li>
                     }
                    <li>
                        <a class="" href="/Sliders/Index/">
                            <i class="fa fa-ellipsis-h"></i>
                            <span>Slider</span>
                        </a>
                    </li>

                    @if (Session["Yetki"].ToString() == "Admin")
                    {

                        <li>
                            <a class="" href="/Services/Index/">
                                <i class="fa fa-money"></i>
                                <span>Services</span>
                            </a>
                        </li>
                    }

                    <li class="sub-menu">
                        <a href="javascript:;" class="">
                            <i class="icon_documents_alt"></i>
                            <span>Blog Entries</span>
                            <span class="menu-arrow arrow_carrot-right"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="/BlogCategorie/Index/">Blog Categories</a></li>
                            <li><a class="" href="/Blog/Index/">Entries</a></li>
                        </ul>
                    </li>

                    @if (Session["Yetki"].ToString() == "Admin")
                    {
                    <li class="sub-menu">
                        <a href="javascript:;" class="">
                            <i class="icon_genius"></i>
                            <span>Carrier</span>
                            <span class="menu-arrow arrow_carrot-right"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="/CarrierCategories/Index/">Carrier Categories</a></li>
                            <li><a class="" href="/Carrier/Index/">Jobs</a></li>
                        </ul>
                    </li>
                     }

                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                @RenderBody()
            </section>
        </section>


        <!--main content end-->
    </section>
    <!-- container section start -->
    <!-- javascripts -->
    @*<script src="/Content/AdminPanelLayout/js/jquery.js"></script>*@
    <script src="/Content/AdminPanelLayout/js/jquery-ui-1.10.4.min.js"></script>
    @*<script src="/Content/AdminPanelLayout/js/jquery-1.8.3.min.js"></script>*@
    <script type="text/javascript" src="/Content/AdminPanelLayout/js/jquery-ui-1.9.2.custom.min.js"></script>
    <!-- bootstrap -->
    <script src="/Content/AdminPanelLayout/js/bootstrap.min.js"></script>
    <!-- nice scroll -->
    <script src="/Content/AdminPanelLayout/js/jquery.scrollTo.min.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery.nicescroll.js" type="text/javascript"></script>
    <!-- charts scripts -->
    <script src="/Content/AdminPanelLayout/assets/jquery-knob/js/jquery.knob.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="/Content/AdminPanelLayout/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="/Content/AdminPanelLayout/js/owl.carousel.js"></script>
    <!-- jQuery full calendar -->
    <
    <script src="/Content/AdminPanelLayout/js/fullcalendar.min.js"></script>
    <!-- Full Google Calendar - Calendar -->
    <script src="/Content/AdminPanelLayout/assets/fullcalendar/fullcalendar/fullcalendar.js"></script>
    <!--script for this page only-->
    <script src="/Content/AdminPanelLayout/js/calendar-custom.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery.rateit.min.js"></script>
    <!-- custom select -->
    <script src="/Content/AdminPanelLayout/js/jquery.customSelect.min.js"></script>
    <script src="/Content/AdminPanelLayout/assets/chart-master/Chart.js"></script>

    <!--custome script for all page-->
    <script src="/Content/AdminPanelLayout/js/scripts.js"></script>
    <!-- custom script for this page-->
    <script src="/Content/AdminPanelLayout/js/sparkline-chart.js"></script>
    <script src="/Content/AdminPanelLayout/js/easy-pie-chart.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery-jvectormap-world-mill-en.js"></script>
    <script src="/Content/AdminPanelLayout/js/xcharts.min.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery.autosize.min.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery.placeholder.min.js"></script>
    <script src="/Content/AdminPanelLayout/js/gdp-data.js"></script>
    <script src="/Content/AdminPanelLayout/js/morris.min.js"></script>
    <script src="/Content/AdminPanelLayout/js/sparklines.js"></script>
    <script src="/Content/AdminPanelLayout/js/charts.js"></script>
    <script src="/Content/AdminPanelLayout/js/jquery.slimscroll.min.js"></script>
    <script>
        //knob
        $(function () {
            $(".knob").knob({
                'draw': function () {
                    $(this.i).val(this.cv + '%')
                }
            })
        });

        //carousel
        $(document).ready(function () {
            $("#owl-slider").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

        /* ---------- Map ---------- */
        $(function () {
            $('#map').vectorMap({
                map: 'world_mill_en',
                series: {
                    regions: [{
                        values: gdpData,
                        scale: ['#000', '#000'],
                        normalizeFunction: 'polynomial'
                    }]
                },
                backgroundColor: '#eef3f7',
                onLabelShow: function (e, el, code) {
                    el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
                }
            });
        });
    </script>

</body>
</html>